 %%%
module PrimitiveTypes

sorts

  PrimType NumType IntType FloatType

context-free syntax

  PrimType = NumType
  PrimType.Boolean = <boolean>
  NumType = IntType
  NumType = FloatType
  IntType.Byte = <byte>
  IntType.Short = <short>
  IntType.Int = <int>
  IntType.Long = <long>
  IntType.Char = <char>
  FloatType.Float = <float>
  FloatType.Double = <double>
