module types-primitive

language Java

start symbol File

test integer literal [[ 
	class Test {
		int m() {
			return [[1]];
		}
	}
]] run get-type to Int()

test integer type [[ 
	class Test {
		[[int]] m() {
			return 1;
		}
	}
]] run get-type to Int()


test float literal [[ 
	class Test {
		float m() {
			return [[1.0]];
		}
	}
]] run get-type to Double()

test float type [[ 
	class Test {
		[[float]] m() {
			return 1;
		}
	}
]] run get-type to Float()
